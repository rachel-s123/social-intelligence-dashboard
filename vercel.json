{
  "version": 2,
  "buildCommand": "CI=false npm run build",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/reports/(.*)",
        "dest": "/reports/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
} 