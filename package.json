  "scripts": {
    "start": "react-scripts start",
    "build": "CI=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "node server.js",
    "dev-server": "npm run build && npm run server",
    "generate-all": "npm run generate-market-recommendations && npm run generate-executive-summaries && npm run generate-conversation-data && npm run generate-competitor-data && npm run generate-wri-data && npm run generate-attribute-resonance && npm run generate-market-intro && npm run generate-r12gs-consumer-data",
