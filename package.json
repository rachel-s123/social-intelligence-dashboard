{
  "name": "bmw-electric-segment-analysis",
  "version": "0.1.0",
  "private": true,

  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.13.0",
    "@mui/material": "^5.13.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "highlight.js": "^11.11.1",
    "openai": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-scripts": "5.0.1",
    "recharts": "^2.15.4",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run build && npm run server",
    "build": "CI=false react-scripts build",
    "server": "node server.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate-all": "npm run generate-r12gs-consumer-data",
    "generate-r12gs-consumer-data": "node scripts/generate_r12gsConsumerData.js",
    "collect-social-data": "node scripts/social-media-apis.js",
    "process-social-data": "node scripts/data-processor.js",
    "weekly-refresh": "node scripts/weekly-refresh.js",
    "weekly-refresh-force": "node scripts/weekly-refresh.js --force",
    "weekly-refresh-status": "node scripts/weekly-refresh.js --status",
    "test-reddit": "node scripts/test-reddit-api.js",
    "debug-reddit": "node scripts/debug-reddit-api.js",
    "test-api-key": "node scripts/test-api-key.js",
    "test-facebook": "node scripts/test-facebook-api.js",
    "clean": "rm -rf build && rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "lint": "eslint src/**/*.{js,jsx}",
    "format": "prettier --write \"src/**/*.{js,jsx,css,md}\"",
    "vercel-build": "CI=false GENERATE_SOURCEMAP=false npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-loop-func": "warn",
      "no-mixed-operators": "warn",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
