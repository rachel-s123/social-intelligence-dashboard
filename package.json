    "generate-market-intro": "node scripts/generate_marketIntro.js",
    "generate-r12gs-consumer-data": "node scripts/generate_r12gsConsumerData.js",
    "clean": "rm -rf build && rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "dev": "npm run generate-all && npm start",
    "lint": "eslint src/**/*.{js,jsx}",
    "format": "prettier --write \"src/**/*.{js,jsx,css,md}\"",
    "vercel-build": "CI=false GENERATE_SOURCEMAP=false npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-loop-func": "warn",
      "no-mixed-operators": "warn",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
